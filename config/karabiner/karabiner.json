    {
      "complex_modifications": {
        "parameters": {
          "basic.to_if_alone_timeout_milliseconds": 1000
        },
        "rules": [
          {
            "description": "Change Capslock to Left_control + Fn if pressed with other keys (Post Esc when pressed alone)",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "fn",
                    "modifiers": [
                      "left_control"
                    ]
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Change Left_control + Fn + I/J/K/L to Arrow Keys",
            "manipulators": [
              {
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": [
                      "left_control",
                      "fn"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_arrow"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": [
                      "left_control",
                      "fn"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "down_arrow"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "i",
                  "modifiers": {
                    "mandatory": [
                      "left_control",
                      "fn"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "up_arrow"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "l",
                  "modifiers": {
                    "mandatory": [
                      "left_control",
                      "fn"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "right_arrow"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Both shifts for capslock",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "left_shift",
                  "modifiers": {
                    "mandatory": ["right_shift"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{
                  "key_code": "caps_lock",
                  "modifiers": [""]
                }
              ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "right_shift",
                  "modifiers": {
                    "mandatory": ["left_shift"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{
                  "key_code": "caps_lock",
                  "modifiers": [""]
                }
              ]
              }
            ]
          }
        ]
      },
      "devices": [],
      "fn_function_keys": {
        "f1": "display_brightness_decrement",
